# Скопируйте в .env и заполните значения
# cp .env.example .env

# Moscow time — используется для расписания
TZ=Europe/Moscow

# Jamendo (devportal.jamendo.com)
JAMENDO_CLIENT_ID=your_client_id

# Groq
GROQ_API_KEY=your_groq_key

# TTS: edge | elevenlabs
TTS_PROVIDER=edge
ELEVENLABS_API_KEY=

# Weather (WeatherAPI.com)
WEATHER_API_KEY=

# Icecast
ICECAST_HOST=localhost
ICECAST_PORT=8000
ICECAST_MOUNT=stream
ICECAST_PASSWORD=your_icecast_password
